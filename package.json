{
	"name": "node-pdu",
	"version": "2.0.0",
	"description": "Creates and parses SMS PDU strings",
	"license": "ISC",
	"author": "jackkum",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/jackkum/node-pdu.git"
	},
	"homepage": "https://github.com/jackkum/node-pdu",
	"dependencies": {
		"moment": "2.29.4"
	},
	"devDependencies": {
		"@types/node": "12.12.6",
		"@typescript-eslint/eslint-plugin": "5.54.0",
		"@typescript-eslint/parser": "5.54.0",
		"eslint": "8.35.0",
		"typedoc": "0.23.26",
		"typescript": "4.9.5"
	},
	"scripts": {
		"build": "yarn build:lib && yarn build:docs",
		"build:test": "tsc ./testTS/index.ts --outDir test",
		"build:lib": "tsc",
		"build:docs": "typedoc --entryPoints src/index.ts --out docs --excludePrivate --customCss ./typedoc.css",
		"lint": "eslint src --ext .ts",
		"prepack": "yarn build:lib"
	},
	"keywords": [
		"pdu",
		"sms"
	]
}
